{
  "info": {
    "name": "BusBudd Transport API",
    "description": "Complete API collection for the BusBudd Transport Management Dashboard.\n\nBase URL: {{baseUrl}}/api\n\nAll endpoints require the /api prefix which the ApiClient adds automatically.\nAuthentication uses Bearer token in the Authorization header.",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
    "_exporter_id": "busbudd"
  },
  "variable": [
    { "key": "baseUrl", "value": "http://localhost:3000", "type": "string" },
    { "key": "token", "value": "", "type": "string" },
    { "key": "schoolId", "value": "S1", "type": "string" },
    { "key": "driverId", "value": "D1", "type": "string" },
    { "key": "routeId", "value": "R1", "type": "string" },
    { "key": "tripId", "value": "T1", "type": "string" },
    { "key": "studentId", "value": "ST1", "type": "string" },
    { "key": "assignmentId", "value": "A1", "type": "string" },
    { "key": "shiftId", "value": "SH1", "type": "string" },
    { "key": "notificationId", "value": "N1", "type": "string" }
  ],
  "auth": {
    "type": "bearer",
    "bearer": [{ "key": "token", "value": "{{token}}", "type": "string" }]
  },
  "item": [
    {
      "name": "Auth",
      "description": "Authentication endpoints for login, logout, session management, and password recovery.",
      "item": [
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/api/auth/login",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"admin@busbudd.com\",\n  \"password\": \"admin123\"\n}"
            }
          },
          "response": [{
            "name": "Success",
            "status": "OK",
            "code": 200,
            "body": "{\n  \"user\": {\n    \"id\": \"U1\",\n    \"name\": \"Admin User\",\n    \"email\": \"admin@busbudd.com\",\n    \"role\": \"SUPER_ADMIN\",\n    \"avatarUrl\": \"\"\n  },\n  \"token\": \"mock-jwt-token\",\n  \"refreshToken\": \"mock-refresh-token\"\n}"
          }]
        },
        {
          "name": "Logout",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/api/auth/logout"
          }
        },
        {
          "name": "Get Current User",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/api/auth/me"
          }
        },
        {
          "name": "Forgot Password",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/api/auth/forgot-password",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"admin@busbudd.com\"\n}"
            }
          }
        },
        {
          "name": "Verify OTP",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/api/auth/verify-otp",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"admin@busbudd.com\",\n  \"otp\": \"123456\"\n}"
            }
          }
        },
        {
          "name": "Reset Password",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/api/auth/reset-password",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"admin@busbudd.com\",\n  \"otp\": \"123456\",\n  \"newPassword\": \"newPassword123\"\n}"
            }
          }
        },
        {
          "name": "Refresh Token",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/api/auth/refresh-token",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"refreshToken\": \"mock-refresh-token\"\n}"
            }
          }
        }
      ]
    },
